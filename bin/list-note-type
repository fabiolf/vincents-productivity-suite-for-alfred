#!/usr/bin/env ruby
#
# Returns the type of note used in the area of focus:
# - bear
# - markdown-notes
# - none

$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'vps'

area = Config.load.focused_area
if area[:bear]
  print 'bear'
elsif area[:markdown_notes]
  print 'markdown-notes'
else
  print 'none'
end
