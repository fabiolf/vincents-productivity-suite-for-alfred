#!/usr/bin/env ruby
#
# Lists all OmniFocus projects for this area of responsibility

$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'vps'

VPS::Alfred.filter do
  triggered_as_snippet = !ENV['TRIGGERED_AS_SNIPPET'].nil?
  VPS::OmniFocusPlugin.projects(triggered_as_snippet)
end
