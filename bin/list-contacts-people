#!/usr/bin/env ruby
#
# Lists all Contacts for this area of responsibility

$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'vps'

puts VPS::OutputFormatter::Alfred.format {
  triggered_as_snippet = !ENV['TRIGGERED_AS_SNIPPET'].nil?
  VPS::Contacts.people(triggered_as_snippet)
}
